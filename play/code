<template>
  <fieldset>
    <legend>defineFactory</legend>

    <div>
      <h1>{{ factory.name }}</h1>
      <input v-model="factory.name" type="text" />
    </div>
  </fieldset>
</template>

<script setup lang="ts">
interface IFormData {
  name: string
  age: number
}

defineFactory(
  {
    children: {
      form: {
        data: {} as IFormData
      }
    }
  },
  {
    mounted() {
      this
    }
  }
)
</script>
