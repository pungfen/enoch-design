{
  "author": "pung <fp94214@gmail.com>",
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C play dev",
    "build": "echo 'hello'",
    "build:cli": "pnpm -C packages/cli build",
    "build:co": "pnpm -C packages/components build",
    "build:f": "pnpm -C packages/factory build",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm build && changeset publish",
    "docs:dev": "pnpm -C packages/docs dev",
    "docs:build": "pnpm -C packages/docs build",
    "format": "prettier --write .",
    "test": "vitest -u"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@enochfe/prettier-config": "workspace:*",
    "@enochfe/test-utils": "workspace:*",
    "@enochfe/tsconfig": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.21",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/ui": "^0.24.1",
    "@vue/test-utils": "^2.1.0",
    "commitizen": "^4.2.5",
    "husky": "^8.0.1",
    "inquirer": "^9.1.2",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.6.5",
    "vitest": "^0.23.4"
  },
  "prettier": "@enochfe/prettier-config"
}
