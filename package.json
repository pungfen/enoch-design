{
  "author": "pung <fp94214@gmail.com>",
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "type": "module",
  "workspaces": [
    "cli",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C play dev",
    "build:cli": "pnpm -C cli build",
    "build": "pnpm -C packages/factory build",
    "cs": "changeset",
    "publish": "pnpm build && changeset publish",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.0",
    "@enochfe/prettier-config": "workspace:*",
    "@enochfe/test-utils": "workspace:*",
    "@enochfe/tsconfig": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.2",
    "@types/prettier": "^2.7.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/ui": "^0.24.3",
    "@vue/test-utils": "^2.1.0",
    "commitizen": "^4.2.5",
    "jsdom": "^20.0.1",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "turbo": "^1.5.6",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "prettier": "@enochfe/prettier-config",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "vue",
        "rollup"
      ]
    }
  }
}
