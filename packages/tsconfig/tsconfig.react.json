{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "react.js",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "resolveJsonModule": true
  }
}
